
<%- unless @exhibit_navigation_data.nil? || @exhibit_navigation_data.empty? -%>
  <%- @exhibit_navigation_data.each do |exhibit| -%>
    <%- if @exhibit && exhibit.set_number == @exhibit.set_number  %>
      <%- unless exhibit.browse_levels.nil? || exhibit.browse_levels.empty? -%>
        <%= render :partial => 'atrium_showcases/navigation_browse_levels', :locals=>{:exhibit_number=>exhibit.set_number, :browse_levels=>exhibit.browse_levels, :browse_facets=>exhibit.browse_facet_names}%>
      <%- end -%>
    <%- end -%>
  <%- end -%>
<%- end -%>

<%- if current_user && is_editing? -%>
  <%- if !@atrium_showcase -%>
    <%= link_to "Customize this page", get_customize_page_path, :class=>"customize"%>
  <%- else %>
    <%= link_to 'View this page',  atrium_showcase_refresh_path(@atrium_showcase.id, :f=>params[:f]) %>
  <%- end %>
<%- end %>



